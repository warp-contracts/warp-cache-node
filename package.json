    "@koa/router": "12.0.0",
    "arweave": "1.11.8",
    "better-sqlite3": "^8.0.1",
    "bullmq": "^3.2.5",
    "dotenv": "^16.0.3",
    "ioredis": "^5.2.4",
    "jsonpath-plus": "^7.2.0",
    "knex": "^2.2.0",
    "knex-paginate": "^3.0.2",
    "koa": "^2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-compress": "^5.1.0",
    "node-nlp": "^4.24.0",
    "safe-stable-stringify": "^2.4.1",
    "warp-contracts": "1.2.35",
    "warp-contracts-evaluation-progress-plugin": "1.0.17",
    "warp-contracts-lmdb": "1.1.5",
    "warp-contracts-nlp-plugin": "1.0.7",
